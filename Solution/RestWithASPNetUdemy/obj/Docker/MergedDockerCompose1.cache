services:
  rest-with-asp-net-udemy:
    build:
      context: C:\Projetos\2022\RestWithASPNetUdemy\Solution\RestWithASPNetUdemy
      dockerfile: RestWithASPNetUdemy/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: RestWithASPNetUdemy
      target: base
    container_name: RestWithASPNetUdemy
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: rest-with-asp-net-udemy:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/RestWithASPNetUdemy.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 44300
      target: 80
    tty: true
    volumes:
    - C:\Projetos\2022\RestWithASPNetUdemy\Solution\RestWithASPNetUdemy\RestWithASPNetUdemy:/app:rw
    - C:\Users\eddop\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\eddop\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\eddop\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\eddop\.nuget\packages:/root/.nuget/packages:ro
    - C:\Projetos\2022\RestWithASPNetUdemy\Solution\RestWithASPNetUdemy:/src:rw
version: '3.4'